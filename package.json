{
  "name": "tpl_nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": " chmod 777 ./src/commons/scripts/setHttps.sh && ./src/commons/scripts/setHttps.sh && dotenv -e .env.loc next dev",
    "build:loc": "dotenv -e .env.loc next build",
    "build:dev": "dotenv -e .env.dev next build",
    "build:main": "dotenv -e .env.main next build",
    "build:all": "yarn build:loc && yarn build:dev && yarn build:main",
    "start:loc": "dotenv -e .env.loc next start",
    "start:dev": "dotenv -e .env.dev next start",
    "start:main": "dotenv -e .env.main next start",
    "lint": "next lint --no-cache --dir src",
    "prettier": "prettier --write src/**/*.{js,ts,jsx,tsx,json,css}"
  },
  "dependencies": {
    "@types/node": "20.3.2",
    "@types/react": "18.2.14",
    "@types/react-dom": "18.2.6",
    "eslint-config-next": "13.4.7",
    "next": "13.4.7",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.60.1",
    "@typescript-eslint/parser": "^5.60.1",
    "dotenv": "^16.3.1",
    "dotenv-cli": "^7.2.1",
    "dotenv-expand": "^10.0.0",
    "eslint": "^8.43.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.8.8",
    "typescript": "^5.1.3"
  },
  "packageManager": "yarn@3.6.0"
}
